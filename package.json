{
  "name": "vite-plugin-css-modules",
  "version": "1.0.0",
  "description": "vitejs css modules 名称自动转化插件",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "build": "del dist && cross-env NODE_ENV=production rollup -c"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zebing/vite-plugin-css-modules.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "babel",
    "vite",
    "react",
    "css modules"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zebing/vite-plugin-css-modules/issues"
  },
  "homepage": "https://github.com/zebing/vite-plugin-css-modules#readme",
  "peerDependencies": {
    "@babel/core": "^7.12.10"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-url": "^6.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.1",
    "rollup": "^2.38.2"
  }
}
